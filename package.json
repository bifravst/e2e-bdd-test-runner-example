{
  "name": "@bifravst/e2e-bdd-test-runner-example",
  "version": "0.0.0-development",
  "description": "Example use of @bifravst/e2e-bdd-test-runner",
  "main": "index.js",
  "scripts": {
    "test": "node dist/feature-runner/run-features.js",
    "prettier": "prettier --write \"./**/*.{ts,js,json}\"",
    "prettier:lint": "prettier --list-different \"./**/*.{ts,js,json}\"",
    "lint": "npm run tslint && npm run prettier:lint",
    "tslint": "eslint --ext .js,.ts \"./{feature-runner,aws}/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/e2e-bdd-test-runner-example.git"
  },
  "keywords": [
    "bdd",
    "aws",
    "cloud",
    "cloudnative",
    "gherkin",
    "testing"
  ],
  "author": "Markus Tacker <m@coderbyheart.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bifravst/e2e-bdd-test-runner-example/issues"
  },
  "homepage": "https://github.com/bifravst/e2e-bdd-test-runner-example#readme",
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "1.83.0",
    "@aws-cdk/aws-codebuild": "1.83.0",
    "@aws-cdk/aws-codepipeline": "1.83.0",
    "@aws-cdk/aws-iam": "1.83.0",
    "@aws-cdk/aws-lambda": "1.83.0",
    "@aws-cdk/aws-logs": "1.83.0",
    "@aws-cdk/aws-s3": "1.83.0",
    "@aws-cdk/aws-sqs": "1.83.0",
    "@aws-cdk/aws-ssm": "1.83.0",
    "@aws-cdk/core": "1.83.0",
    "@aws-sdk/client-cloudformation": "3.2.0",
    "@bifravst/cloudformation-helpers": "5.0.0",
    "@bifravst/code-style": "8.0.134",
    "@bifravst/e2e-bdd-test-runner": "5.0.10",
    "@bifravst/eslint-config-typescript": "5.0.86",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@types/node": "14.14.20",
    "aws-cdk": "1.83.0",
    "eslint": "7.17.0",
    "husky": "4.3.7",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "release": {
    "branch": "saga",
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  }
}
